{
  "name": "formspec-workspace",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript monorepo workspace",
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck",
    "api-extractor": "pnpm -r run api-extractor",
    "api-extractor:local": "pnpm -r run api-extractor:local",
    "api-documenter": "pnpm -r run api-documenter",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@api-extractor-tools/declaration-file-normalizer": "^0.1.0-alpha.6",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.18.0",
    "@microsoft/api-documenter": "^7.26.10",
    "@microsoft/api-extractor": "^7.55.2",
    "@types/node": "^22.13.0",
    "@types/react": "^19.2.9",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.0",
    "react": "^19.2.3",
    "tsd": "^0.31.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "zod": "^3.24.0"
  },
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "@types/eslint": "^9.6.1"
    }
  }
}
