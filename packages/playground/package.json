{
  "name": "@formspec/playground",
  "version": "0.1.0-alpha.0",
  "private": true,
  "description": "Interactive playground for FormSpec - write forms and see generated schemas live",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@formspec/build": "workspace:*",
    "@formspec/constraints": "workspace:*",
    "@formspec/core": "workspace:*",
    "@formspec/dsl": "workspace:*",
    "@formspec/eslint-plugin": "workspace:*",
    "@jsonforms/core": "^3.7.0",
    "@jsonforms/material-renderers": "^3.7.0",
    "@jsonforms/react": "^3.7.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.0",
    "@mui/material": "^7.3.0",
    "@typescript-eslint/parser": "^8.55.0",
    "allotment": "^1.20.2",
    "eslint": "^9.39.2",
    "monaco-editor": "^0.52.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "typescript": "^5.7.3",
    "vite": "^6.3.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "formspec",
    "playground",
    "jsonschema",
    "jsonforms"
  ],
  "license": "UNLICENSED"
}
